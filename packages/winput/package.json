{
  "name": "winput",
  "version": "1.0.8",
  "description": "Windows automation library - keyboard, mouse, window, screen, image, and overlay control",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "bun build src/index.ts --outdir=dist --target=bun --format=esm --minify && bunx tsc --emitDeclarationOnly --outDir dist",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "windows",
    "automation",
    "keyboard",
    "mouse",
    "screen",
    "window",
    "image",
    "overlay",
    "bun",
    "ffi"
  ],
  "author": "handy-z",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/handy-z/winput.git"
  },
  "bugs": {
    "url": "https://github.com/handy-z/winput/issues"
  },
  "homepage": "https://github.com/handy-z/winput#readme",
  "engines": {
    "bun": ">=1.0.0"
  },
  "os": [
    "win32"
  ],
  "dependencies": {
    "@winput/keyboard": "latest",
    "@winput/mouse": "latest",
    "@winput/screen": "latest",
    "@winput/window": "latest",
    "@winput/image": "latest",
    "@winput/overlay": "latest",
    "@winput/utils": "latest"
  }
}
